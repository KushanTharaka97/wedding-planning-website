<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.controller.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>com.controller.usignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>spsignup</servlet-name>
        <servlet-class>com.controller.spsignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>usersign</servlet-name>
        <servlet-class>com.controller.usersign</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login1</servlet-name>
        <servlet-class>com.controller.login1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>request</servlet-name>
        <servlet-class>com.requestController.request</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>detailedView</servlet-name>
        <servlet-class>MainServices.sevices.detailedView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>acceptUser</servlet-name>
        <servlet-class>com.requestController.acceptUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminDelete</servlet-name>
        <servlet-class>com.controller.adminalPanal.adminDelete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminUpdate</servlet-name>
        <servlet-class>com.controller.adminalPanal.adminUpdate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>spsignup</servlet-name>
        <url-pattern>/spsignup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>usersign</servlet-name>
        <url-pattern>/usersign</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login1</servlet-name>
        <url-pattern>/login1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>request</servlet-name>
        <url-pattern>/request</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>detailedView</servlet-name>
        <url-pattern>/detailedView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>acceptUser</servlet-name>
        <url-pattern>/acceptUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminDelete</servlet-name>
        <url-pattern>/adminDelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminUpdate</servlet-name>
        <url-pattern>/adminUpdate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
